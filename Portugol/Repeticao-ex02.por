programa {
  funcao inicio() {
    /*Peça ao usuário um valor menor que 10. Se o número não estiver dentro desse intervalo, repita a leitura até que seja digitado um valor válido. */

    inteiro numero

    

    faca{
      escreva("Insira um valor menor que 10: ")
      leia (numero)
    }enquanto (numero >= 10)
  }
}
